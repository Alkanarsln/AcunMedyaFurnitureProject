@model Team
@{
    ViewData["Title"] = "Furniter Ekibi Güncelleme Sayfası";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<form method="post">
    <input type="hidden" asp-for="TeamID" />
    <label>İsim Soyisim</label>
    <input type="text" asp-for="NameSurname" class="form-control" />
    <br /><br>
    <label>Resim Url</label>
    <input type="text" asp-for="ImageUrl" class="form-control" />
    <br /><br />
    <label>Unvan</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br /><br />
    <label>Yorum</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br /><br />
    <button class="btn btn-outline-primary">Güncelle</button>

</form>

