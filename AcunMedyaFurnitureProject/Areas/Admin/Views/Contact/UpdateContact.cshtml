@model ContactInfo
@{
    ViewData["Title"] = "İletişim Bilgisi Güncelleme Sayfası";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="ContactInfoID" />
    <label>Adres</label>
    <input type="text" asp-for="Address" class="form-control" />
    <br /><br>
    <label>Telefon</label>
    <input type="text" asp-for="Phone" class="form-control" />
    <br /><br />
    <label>Email</label>
    <input type="email" asp-for="Email" class="form-control" />
    <br /><br />
    <button class="btn btn-outline-primary">Güncelle</button>

</form>


